 251 # If you are using any other location directive and 'audio-only=true' in the query parameter for AMS 5.0,
 252 # please replace hls-live or hls-vod with that location directive below
 253 RewriteEngine On
 254 RewriteCond %{QUERY_STRING} audio-only=true
 255 RewriteCond %{QUERY_STRING} codec=aac
 256 RewriteRule ^/hls-vod/(.+).m3u8$ /hls-vod/audio-only-aac/$1.m3u8? [R=301,L]
 257 RewriteCond %{QUERY_STRING} audio-only=true
 258 RewriteRule ^/hls-vod/(.+).m3u8$ /hls-vod/audio-only/$1.m3u8? [R=301,L]
 259 RewriteCond %{QUERY_STRING} audio-only=true
 260 RewriteCond %{QUERY_STRING} codec=aac
 261 RewriteRule ^/hls-live/(.+).m3u8$ /hls-live/audio-only-aac/$1.m3u8? [R=301,L]
 262 RewriteCond %{QUERY_STRING} audio-only=true
 263 RewriteRule ^/hls-live/(.+).m3u8$ /hls-live/audio-only/$1.m3u8? [R=301,L]
