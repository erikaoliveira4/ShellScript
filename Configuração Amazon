 213 LoadModule cache_module modules/mod_cache.so
 214 <IfModule mod_cache.c>
 215     LoadModule disk_cache_module modules/mod_disk_cache.so
 216     <IfModule mod_disk_cache.c>
 217         CacheEnable disk /hds-vod
 218         CacheEnable disk /hls-vod
 219             CacheEnable disk /hds-live
 220             CacheEnable disk /hls-live
 221         CacheRoot /opt/adobe/ams/Apache2.2/cacheroot
 222                 CacheLockPath /tmp/mod_cache-lock-ams
 223         CacheMaxFileSize 10000000
 224         CacheLock On
 225     </IfModule>
 226 </IfModule>
