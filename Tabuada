#!/bin/bash

echo -n "Digite um numero para fazer a tabuada: "
read valor_digitado

$valor_digitado;
        for i in $(seq 1 10);
                do echo $i x $valor_digitado = $(($i * $valor_digitado)) ;
                        done

#a=10;for i in $(seq 1 10); do echo $i x $a = $(($i * $a)) ;done


